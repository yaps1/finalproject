<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="com/example/demo/db/db.properties"/>
<typeAliases>
	<typeAlias type="com.example.demo.vo.GoodsDetailVO" alias="goodsDetailVO"/>
	<typeAlias type="com.example.demo.vo.GoodsFavorVO" alias="goodsFavorVO"/>
	<typeAlias type="com.example.demo.vo.HotKeywordVO" alias="hotkeywordVO"/>
	<typeAlias type="com.example.demo.vo.GoodsVO" alias="goodsVO"/>
	<typeAlias type="com.example.demo.vo.GBVO" alias="gbVO"/>
	<typeAlias type="com.example.demo.vo.GBJVO" alias="gbjVO"/>
	<typeAlias type="com.example.demo.vo.GBDVO" alias="gbdVO"/>
	<typeAlias type="com.example.demo.vo.GBCVO" alias="gbcVO"/>
	<typeAlias type="com.example.demo.vo.SGVO" alias="sgVO"/>
	<typeAlias type="com.example.demo.vo.SGCVO" alias="sgcVO"/>
	<typeAlias type="com.example.demo.vo.NoticeVO" alias="noticeVO"/>
	<typeAlias type="com.example.demo.vo.MemberVO" alias="memberVO"/>
	<typeAlias type="com.example.demo.vo.RestVO" alias="restVO"/>
	<typeAlias type="com.example.demo.vo.RestReviewVO" alias="restReviewVO"/>
	<typeAlias type="com.example.demo.vo.GoodsReviewVO" alias="goodsReviewVO"/>
	<typeAlias type="com.example.demo.vo.QnaVO" alias="qnaVO"/>
	<typeAlias type="com.example.demo.vo.MemberChartVO" alias="memberChartVO"/>

</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/example/demo/db/noticeMapper.xml"/>
    <mapper resource="com/example/demo/db/qnaMapper.xml"/>
    <mapper resource="com/example/demo/db/hotKeywordMapper.xml"/>
    <mapper resource="com/example/demo/db/goodsMapper.xml"/>
    <mapper resource="com/example/demo/db/groupbuyingMapper.xml"/>
    <mapper resource="com/example/demo/db/smallgroupMapper.xml"/>
    <mapper resource="com/example/demo/db/memberMapper.xml"/>
    <mapper resource="com/example/demo/db/restMapper.xml"/>
    <mapper resource="com/example/demo/db/managerMapper.xml"/>

  </mappers>
</configuration>