<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="com/example/demo/db/db.properties"/>
<typeAliases>
	<typeAlias type="com.example.demo.vo.MemberVO" alias="memberVO"/>
	<typeAlias type="com.example.demo.vo.SGVO" alias="sgVO"/>
	<typeAlias type="com.example.demo.vo.SGCVO" alias="sgcVO"/>
	<typeAlias type="com.example.demo.vo.RestVO" alias="restVO"/>
	<typeAlias type="com.example.demo.vo.RestReviewVO" alias="restReviewVO"/>
</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/example/demo/db/memberMapper.xml"/>
    <mapper resource="com/example/demo/db/sgMapper.xml"/>
    <mapper resource="com/example/demo/db/sgcMapper.xml"/>
    <mapper resource="com/example/demo/db/restMapper.xml"/>
    <mapper resource="com/example/demo/db/restReviewMapper.xml"/>
  </mappers>
</configuration>